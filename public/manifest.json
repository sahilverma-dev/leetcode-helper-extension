{
  "name": "Leetcode Helper Bot",
  "description": "A browser extension designed to assist users in solving LeetCode problems by providing real-time information and tools.",
  "manifest_version": 3,
  "version": "1.0.0",
  "background": {
    "service_worker": "service-worker.js"
  },
  "action": {
    "default_title": "Click to open panel"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },

  "side_panel": {
    "default_path": "index.html"
  },
  "permissions": ["sidePanel", "tabs", "scripting", "storage", "clipboardRead"],
  "host_permissions": ["https://leetcode.com/*"]
}
